
public class TablaEnteros {
	// Datos
	static TablaTradicional clave;
	int[] tabla;

	// Metodos
	public TablaEnteros() {
		tabla = new int[4];
		tabla[0] = 20;
		tabla[1] = 8;
		tabla[2] = 31;
		tabla[3] = 56;
	}

	public void imprimir() {
		for (int i = 0; i < 4; i++)
			System.out.println(tabla[i]);
	}
}
